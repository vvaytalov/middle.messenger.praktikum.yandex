{
    "name": "vvaytalov",
    "version": "1.0.0",
    "description": "vvaytalov chat for yandex",
    "scripts": {
        "build": "parcel build src/static/*.html --no-minify",
        "dev": "npm run watch & node server.js",
        "start": "parcel build src/static/*.html --no-minify && node server.js",
        "watch": "parcel watch build src/static/*.html",
        "stylelint": "stylelint '**/*.css'",
        "lint": "eslint src --ext .js,.ts && npx stylelint \"**/*.css\"",
        "fix": "eslint src --ext .js,.ts --fix && npx stylelint \"**/*.css\" --fix"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@parcel/config-default": "^2.2.1",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.8.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-plugin-import": "^2.25.4",
        "parcel-bundler": "^1.12.5",
        "postcss": "^8.4.7",
        "postcss-calc": "^8.2.4",
        "postcss-nested": "^5.0.6",
        "sass": "^1.49.9"
    },
    "dependencies": {
        "express": "^4.17.2",
        "uuid": "^8.3.2"
    }
}
